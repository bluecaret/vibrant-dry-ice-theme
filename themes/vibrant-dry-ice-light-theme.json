        "source.css support.type.vendored.property-name",
        "source.css.scss support.type.vendored.property-name"
      ],
      "settings": {
        "foreground": "#00a9d3ad",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": ["meta.property-value", "support.constant.property-value"],
      "settings": {
        "foreground": "#13BBB7"
      }
    },
    {
      "name": "CSS: Constant",
      "scope": ["source.css support.constant", "source.css.scss support.constant"],
      "settings": {
        "foreground": "#2DAE58"
      }
    },
    {
      "name": "CSS: Selector punctuation",
      "scope": ["punctuation.definition.entity.css", "keyword.operator.combinator.css"],
      "settings": {
        "foreground": "#6523ff"
      }
    },
    {
      "name": "CSS: Functions",
      "scope": ["source.css support.function", "source.css.scss support.function"],
      "settings": {
        "foreground": "#13BBB7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS: Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "foreground": "#ff018d",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS: Base font color",
      "scope": ["source.css", "source.css.scss"],
      "settings": {
        "foreground": "#F767BB"
      }
    },
    {
      "name": "CSS: Ampersand",
      "scope": ["entity.name.tag.reference.css", "entity.name.tag.reference.scss"],
      "settings": {
        "foreground": "#6523ff",
        "fontStyle": ""
      }
    },

    // Sass Overrides
