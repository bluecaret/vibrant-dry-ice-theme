{
  "name": "Vibrant Dry Ice",
  "type": "light",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#c02323"
    },
    "variable.constant": {
      "foreground": "#0d2f4f"
    },
    "variable.defaultLibrary": {
      "foreground": "#4f0a7b"
    }
  },
  "tokenColors": [
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#FF5C56"
      }
    },
    {
      "name": "Property names",
      "scope": [
        "meta.object-literal.key",
        "meta.object-literal.key constant.character.escape",
        "meta.object-literal string",
        "meta.object-literal string constant.character.escape",
        "support.type.property-name",
        "support.type.property-name constant.character.escape"
      ],
      "settings": {
        "foreground": "#11658F"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "meta.class storage.type",
        "keyword.operator.expression.import",
        "keyword.operator.new",
        "keyword.operator.expression.delete"
      ],
      "settings": {
        "foreground": "#F767BB"
      }
    },
    {
      "name": "Types",
      "scope": [
        "support.type",
        "meta.type.annotation entity.name.type",
        "new.expr meta.type.parameters entity.name.type",
        "storage.type.primitive",
        "storage.type.built-in.primitive",
        "meta.function.parameter storage.type"
      ],
      "settings": {
        "foreground": "#2DAE58"
      }
    },
    {
      "name": "Annotations",
      "scope": ["storage.type.annotation"],
      "settings": {
        "foreground": "#C25193"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#FF5C57CC"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#FF5C57",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#001aff"
      }
    },
    {
      "name": "this Variables",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#13BBB7"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#09A1ED"
      }
    },
    {
      "name": "Decorators",
      "scope": ["entity.name.function.decorator"],
      "settings": {
        "foreground": "#11658F"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "meta.class entity.name.type",
        "new.expr entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#13BBB7"
      }
    },
    {
      "name": "Preprocessors",
      "scope": ["keyword.preprocessor.pragma", "keyword.control.directive.include", "keyword.other.preprocessor"],
      "settings": {
        "foreground": "#11658F"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#FF5C56"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        // "fontStyle": "underline"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": [
        "constant.numeric"
        // "constant.character",
        // "constant"
      ],
      "settings": {
        "foreground": "#FF5C57"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "constant.character"],
      "settings": {
        "foreground": "#2DAE58"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#CF9C00"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#CF9C00"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#F5B900"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": ["string.regexp", "string.regexp constant.character.escape"],
      "settings": {
        "foreground": "#13BBB7"
      }
    },
    {
      "name": "Strings: Regular Expressions Punctuation",
      "scope": [
        "keyword.operator.quantifier.regexp",
        "keyword.operator.negation.regexp",
        "keyword.operator.or.regexp",
        "string.regexp punctuation",
        "string.regexp keyword",
        "string.regexp keyword.control",
        "string.regexp constant",
        "variable.other.regexp"
      ],
      "settings": {
        "foreground": "#00A39F"
      }
    },
    {
      "name": "Strings: Regular Expressions Modifier Flags",
      "scope": ["string.regexp keyword.other"],
      "settings": {
        "foreground": "#00A39F88"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#CF9C00"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#b8b8b8"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "fontStyle": "",
        "foreground": "#9194A2"
      }
    },
    {
      "name": "DocBlock: Type",
      "scope": "comment.block.documentation entity.name.type",
      "settings": {
        "foreground": "#2DAE58"
      }
    },
    {
      "name": "DocBlock: Keyword",
      "scope": [
        "comment.block.documentation storage",
        "comment.block.documentation keyword.other",
        "meta.class comment.block.documentation storage.type"
      ],
      "settings": {
        "foreground": "#9194A2"
      }
    },
    {
      "name": "DocBlock: Variable",
      "scope": ["comment.block.documentation variable"],
      "settings": {
        "foreground": "#C25193"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "punctuation.definition.string"],
      "settings": {
        "foreground": "#969aad",
        "fontStyle": "italic"
      }
    },
    {
      "name": "punctuation.definition.interpolation",
      "scope": ["punctuation.definition.interpolation"],
      "settings": {
        "foreground": "#d04ff7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "punctuation.definition.template-expression",
      "scope": ["punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#d04ff7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator", "keyword.other.arrow", "keyword.control.@"],
      "settings": {
        "foreground": "#d04ff7"
      }
    },

    // HTML Overrides
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.metadata.doctype.html entity.name.tag",
        "meta.tag.metadata.doctype.html entity.other.attribute-name.html",
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#9194A2"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#13BBB7",
        "fontStyle": ""
      }
    },
    {
      "name": "HTML: Tag Names",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#13BBB7",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": ["meta.tag entity.other.attribute-name", "entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#FF8380"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": ["constant.character.entity", "punctuation.definition.entity"],
      "settings": {
        "foreground": "#CF9C00"
      }
    },
    {
      "name": "Quoted strings",
      "scope": ["string.quoted.double", "string.quoted.single"],
      "settings": {
        "foreground": "#282c29"
      }
    },

    // Vue Overrides

    // CSS Overrides
    {
      "name": "CSS: Base font color (just to colorize multi-selector commas correctly)",
      "scope": ["source.css"],
      "settings": {
        "foreground": "#ADB1C2"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation",
        "source.css entity.name.tag",
        "source.css.scss entity.name.tag",
        "source.css.scss entity.other.attribute-name.class.css",
        "source.css.scss entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#f50491"
      }
    },
    {
      "name": "CSS: At-rules",
      "scope": ["keyword.control.at-rule", "keyword.control.at-rule punctuation.definition.keyword"],
      "settings": {
        "foreground": "#C25193"
      }
    },
    {
      "name": "CSS: Variables",
      "scope": ["source.css variable", "source.css.scss variable"],
      "settings": {
        "foreground": "#001aff"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": [
        "source.css meta.property-name",
        "source.css support.type.property-name",
        "source.css.scss meta.property-name",
        "source.css.scss support.type.property-name",
        "source.css.scss entity.name.tag"
      ],
      "settings": {
        "foreground": "#00a9d3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS: Vendored Property Names",
